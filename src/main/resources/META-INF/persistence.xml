<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="performance-reporting" transaction-type="RESOURCE_LOCAL">
		<class>com.test.domain.Branch</class>
		<class>com.test.domain.Build</class>
		<class>com.test.domain.Environment</class>
		<class>com.test.domain.TestCase</class>
		<class>com.test.domain.TestResult</class>
		<class>com.test.domain.TestSuite</class>
		<class>com.test.domain.Run</class>
		<class>com.test.domain.Run_TestCase_TestResult</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/fortest"/>
			<property name="javax.persistence.jdbc.user" value="root"/>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
			<property name="javax.persistence.jdbc.password" value="password"/>
		</properties>
	</persistence-unit>
</persistence>
